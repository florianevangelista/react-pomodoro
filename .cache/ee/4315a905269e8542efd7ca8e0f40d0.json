      "sources": {
        "src/components/Timer.jsx": "import React, { Component } from 'react';\r\n\r\nclass Timer extends Component {\r\n    state = { \r\n        initialValue: 10,\r\n        duration: 10,\r\n        message: '',\r\n        handle: 0\r\n    }\r\n\r\n     componentWillUnmount() {\r\n        this.clean();\r\n    }\r\n\r\n    count = () => {\r\n        this.setState({\r\n            duration: this.state.duration - 1\r\n        });\r\n\r\n        if(this.state.duration === 0) {\r\n            this.clean();\r\n        }\r\n    }\r\n    \r\n    clean = (message) => {\r\n        clearInterval(this.state.handle);\r\n        this.setState({\r\n            message: message\r\n        })\r\n    }\r\n\r\n    start = () => {\r\n        const handle = setInterval(()=>{\r\n            this.count()\r\n        }, 1000);\r\n\r\n        this.setState({\r\n            handle\r\n        });\r\n    };\r\n\r\n    stop = () => {\r\n        this.clean('Mis en pose');\r\n    };\r\n\r\n    reset = () => {\r\n        this.clean();\r\n        this.setState({\r\n            duration: this.state.initialValue\r\n        })\r\n    };\r\n\r\n    render() { \r\n        return (\r\n            <> \r\n                <div>\r\n                    <h2>Pomodor</h2>\r\n                    <h2>{this.state.message}</h2>\r\n                    <div>dur√©e : {this.state.duration}</div>\r\n                </div>\r\n\r\n                <div>\r\n                    <button onClick={this.start}>start</button>\r\n                    <button onClick={this.stop}>stop</button>\r\n                    <button onClick={this.reset}>reset</button>\r\n                </div>\r\n            </>\r\n        );\r\n    }\r\n}\r\n \r\nexport default Timer;"
